    private void tabPanelMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_tabPanelMouseClicked
        if(HYGROCHRON){
            try {
                hygrochron.readHumidity();
                hygrochron.readTemperatures();
               
                try {
                    hygrochron.generateGraph();
                } catch (Exception ex) {  
                }
                BufferedImage image;
                JLabel label;
                try {
                    ImageScaler scaler = new ImageScaler();

                    image = ImageIO.read(new File(hygrochron.HUMIDITY_GRAPH_NAME));
                    label = scaler.getLabel(100, 100, image);
                    jLabel3 = scaler.getLabel(100, 100, image);
                    image = ImageIO.read(new File(hygrochron.TEMPERATURE_GRAPH_NAME));
                    label = scaler.getLabel(100, 100, image);
                    tabPanel1.add(label);

                } catch (IOException ex) {

                }

            } catch (OneWireException ex) {
            }
        }else{
           
        }
    }//GEN-LAST:event_tabPanelMouseClicked

