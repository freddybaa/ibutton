    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton2ActionPerformed

         if(HYGROCHRON)
            try {

            hygrochron.readHumidity();
            hygrochron.readTemperatures();
            try {
                hygrochron.generateGraph();
            } catch (Exception ex) {

            }
            new RealTimeMonitor(hygrochron, hygrochron.TEMP_HUM_GRAPH_NAME);
        } catch (OneWireException ex) {
        }
        else
            try {
            new RealTimeMonitor(thermochron, thermochron.TEMPERATURE_GRAPH_NAME);
        } catch (OneWireException ex) {
            Logger.getLogger(StartWindow.class.getName()).log(Level.SEVERE, null, ex);
        }


    }//GEN-LAST:event_jButton2ActionPerformed

