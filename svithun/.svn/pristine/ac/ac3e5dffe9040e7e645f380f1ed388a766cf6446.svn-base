    private class RealTime implements MouseListener{
        private String path;
        private String title;
        public RealTime(String path, String title){
            this.path = path;
            this.title = title;
        }
        public void mouseClicked(MouseEvent e) {
            if(HYGROCHRON){
                try {
                    hygrochron.readHumidity();
                    hygrochron.readTemperatures();
                    hygrochron.generateGraph();
                    new RealTimeMonitor(hygrochron, path, title);
                } catch (Exception ex) {}
            }else{
                try {
                    thermochron.readTemperatures();
                    thermochron.generateGraph();
                    new RealTimeMonitor(thermochron, path, title);
                } catch (Exception ex) {}
            }
        }

        public void mousePressed(MouseEvent e) {}

        public void mouseReleased(MouseEvent e) {}

        public void mouseEntered(MouseEvent e) {}

        public void mouseExited(MouseEvent e) {}

    }
    private void tabPanel1MouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_tabPanel1MouseClicked
        BufferedImage hum = null,temp = null,hum_temp = null;
        if(HYGROCHRON){
            try {
                thumb2.setVisible(true);
                thumb3.setVisible(true);
                hygrochron.readHumidity();
                hygrochron.readTemperatures();
                hygrochron.generateGraph();

                try {
                    hum = ImageIO.read(new File(hygrochron.HUMIDITY_GRAPH_NAME));
                    temp = ImageIO.read(new File(hygrochron.TEMPERATURE_GRAPH_NAME));
                    hum_temp = ImageIO.read(new File(hygrochron.TEMP_HUM_GRAPH_NAME));
                } catch (IOException ex) {
                    
                }
                ImageScaler scaler = new ImageScaler();
                thumb1.setIcon(scaler.getImage(250, 250, hum));
                thumb1.addMouseListener(new RealTime(hygrochron.HUMIDITY_GRAPH_NAME, "Humidity"));
                thumb2.setIcon(scaler.getImage(250,250,temp));
                thumb2.addMouseListener(new RealTime(hygrochron.TEMPERATURE_GRAPH_NAME,"Temperature"));
                thumb3.setIcon(scaler.getImage(250, 250, hum_temp));
                thumb3.addMouseListener(new RealTime(hygrochron.TEMP_HUM_GRAPH_NAME,"Humidity & Temperature"));
            } catch (Exception ex) {
                
            }

        }else{
            try {
                thumb2.setVisible(false);
                thumb3.setVisible(false);
                thermochron.readTemperatures();
                thermochron.generateGraph();
                System.out.println(thermochron.TEMPERATURE_GRAPH_NAME);

                temp = ImageIO.read(new File(thermochron.TEMPERATURE_GRAPH_NAME));

                ImageScaler scaler = new ImageScaler();
                thumb1.setIcon(scaler.getImage(250, 500, temp));
                thumb1.addMouseListener(new RealTime(thermochron.TEMPERATURE_GRAPH_NAME, "Temperature"));
            } catch (Exception ex) {
                Logger.getLogger(StartWindow.class.getName()).log(Level.SEVERE, null, ex);
            }
        }
    }//GEN-LAST:event_tabPanel1MouseClicked

